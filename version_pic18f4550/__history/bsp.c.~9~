#ifndef _BSP_C
#def _BSP_C

#include <18f4550.h>
#include <usb_bootloader.h> // Necesario por el bootloader
#use delay(clock=20M)
#fuses HS,NOWDT,NOPROTECT,NOLVP
#include <select_version.c>
#include <hal.c>

#define OUTPUT 0x00
#define INPUt 0xFF
#define BTN1 PIN_B0

enum pines{
   D0,D1,D2,D3,D4,D5,D6,D7,
   B0,B1,B2,B3,B4,B5,B6,B7,
   C0,C1,C2,C3,C4,C5,C6,C7,
};

#endif
